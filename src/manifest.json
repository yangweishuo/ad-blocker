{
  "manifest_version": 3,
  "name": "Ad Blocker Pro",
  "version": "1.0.0",
  "description": "A powerful ad blocker with EasyList support and custom rules",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
} 